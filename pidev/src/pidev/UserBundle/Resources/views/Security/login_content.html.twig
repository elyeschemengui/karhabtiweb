{% trans_default_domain 'FOSUserBundle' %}

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}
<br><br>

<form action="{{ path("fos_user_security_check") }}" method="post">
    {% if csrf_token %}
        <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
    {% endif %}

        <input type="hidden" name="_target_path" value="/check" />

    <div class="main-agileits">
        <!--form-stars-here-->
        <div class="form-w3-agile" style="margin-top: 115px;">
            <h2>S'identifier</h2>
                <div class="form-sub-w3">
    <label for="username">USERNAME</label>
    <input type="text" id="username" name="_username" value="{{ last_username }}" required="required" />
                    <div class="icon-w3" style="margin-right: 420px;">
                        <i class="fa fa-user" aria-hidden="true"></i>
                    </div>
                </div>
                <div class="form-sub-w3">
    <label for="password">PASSWORD</label>
    <input type="password" id="password" name="_password" required="required" />
                    <div class="icon-w3" style="margin-right: 420px;">
                        <i class="fa fa-unlock-alt" aria-hidden="true"></i>
                    </div>
                </div>
            <p class="p-bottom-w3ls"> <a class="w3_play_icon1" href="{{ path('fos_user_resetting_request') }}">Mot de passe oublié </a></p><br>

            <p class="p-bottom-w3ls">Vous êtes nouveau sur K A R H A B T Y ? <a class="w3_play_icon1" href="{{ path('fos_user_registration_register') }}">S'inscrire </a></p>


            <div class="submit-w3l">

   <input type="submit" id="_submit" name="_submit" value="{{ 'Submit'|trans }}" />

                </div>

        </div>
    </div>
</form>
